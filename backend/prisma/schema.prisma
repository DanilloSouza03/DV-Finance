generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Client {
  id     Int     @id @default(autoincrement())
  name   String
  email  String  @unique
  active Boolean @default(true)
}